<h1>Users</h1>
<table>
	<% @users.each do |user| %>
	<tr>
		<th>image</th>
		<th>name</th>
		<th></th>
	</tr>
	<tr>
		<td><%= attachment_image_tag @user, :profile_image, :fill,60,60, format: 'jpeg', fallback: "no_image.jpg" %>
		</td>
		<td><%= user.name %></td>
		<td><%= link_to "Show", user_path(user.id) %></td>
	</tr>
	<% end %>
</table>